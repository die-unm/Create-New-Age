  "authors": [
    "Antarctic Gardens"
  ],
  "contact": {
    "homepage": "http://antarcticgardens.org/",
    "sources": "https://gitlab.com/antarcticgardens/create-new-age",
    "issues": "https://gitlab.com/antarcticgardens/create-new-age/-/issues"
  },
  "license": "BSD-3-Clause",
